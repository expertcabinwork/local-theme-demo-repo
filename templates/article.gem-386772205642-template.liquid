                  {% endfor %}
                </ul>                {% if paginate.pages > 1 %}
                  {% include 'pagination' %}
                {% endif %}              {% endif %}              <hr class="hr--large">              <h2 class="h3 text-center">{{ 'blogs.comments.title' | t }}</h2>
              <hr class="hr--small hr--clear">              <div class="form-vertical">
                {% form 'new_comment', article %}                  {{ form.errors | default_errors }}                  <div class="grid grid--small">
