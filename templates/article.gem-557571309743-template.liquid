                    </div>
                  </div>                  <label for="CommentBody" class="hidden-label">{{ 'blogs.comments.message' | t }}</label>
                  <textarea class="input-full{% if form.errors contains 'body' %} error{% endif %}" name="comment[body]" id="CommentBody" placeholder="{{ 'blogs.comments.message' | t }}">{{ form.body }}</textarea>                  <div class="text-center">
                    {% if blog.moderated? %}
                      <p>{{ 'blogs.comments.moderated' | t }}</p>
                    {% endif %}                    <input type="submit" class="btn" value="{{ 'blogs.comments.post' | t }}">
                  </div>                {% endform %}
              </div>            </div>
          {% endpaginate %}
        {% endif %}      </div>
    </div>  </article>
</div><hr class="hr--medium hr--clear">
