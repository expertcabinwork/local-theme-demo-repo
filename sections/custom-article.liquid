{{ 'custom-article-section.css' | asset_url | stylesheet_tag }}
{{ 'table-section.css' | asset_url | stylesheet_tag }}

{%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  .table-of-content__main{
    padding-bottom: 16px 
  }
  .table-of-content__inner a{
    display:block;
    text-transform: capitalize;
    color: rgb(36, 85, 166);
    font-size: 16px;
    font-weight: 600;
    display: flex;
    align-items: center;
  }
  .table-of-content__inner a img{
    width: 14px;
    height: 13px;
    object-fit: contain;
    margin-right: 18px;

  }
  .table-of-content__main h4{
    text-transform: uppercase;
    color: #3154a1;
    border-bottom: 1px solid #242424;
    padding-bottom: 10px 
  }
  /* .table-of-content__inner a::before{
    content: "";
      display: inline-block;
      width: 13px;
      height: 14px ;
      background: url('{{ 'play-button.png' | asset_url }}') no-repeat center center;
      background-size: cover; 
      margin-right: 10px;
      position: relative;
      top: 2px;
      left: 0px;
  } */
  @media screen and (max-width: 768px) {
    .table-of-content__inner a{
      font-size:13px;
      margin-bottom: 2px;
       align-items:unset;
    }
    .table-of-content__inner a img{
      position: relative;
      top: 5px;
    }
  }
  .key-takeaways__main{
    margin: 16px 0;
  }
  .key-takeaways__container{
    background: #cccccc45;
    padding: 18px;
    border-radius: 24px;
    box-shadow: 1px 2px 6px 1px #ccc;
  }
  .key-takeaways__container h4{
    color: #3154a1;
  }
  .key-content li, .key-content p, .key-content span, key-content a{
    color: rgb(49, 84, 161);
  }
  .key-content li::before {
    content: url('{{ 'play-button2.png' | asset_url }}');
    margin-right: 5px; /* Adjust the spacing between the icon and text */
    display: block;
    top: 2px;
    position: relative;
}
  .key-content li{
    display: flex;
  }
  .key-content li 
  {%- endstyle -%}
<div class="custom_article_section section-{{ section.id }}-padding ">
  <div class="container--large">
    <div class="main_custom_article">
      <div class=" {% if section.settings.sidebar_enable %}left_custom_article{% endif %}">
   
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when 'key-takeaways'  -%}
            <div class="key-takeaways__main">
            <div class="key-takeaways__container">
            {% if block.settings.key_title != blank %}
            <h4>{{ block.settings.key_title }}</h4>
            {% endif %}  
            {% if block.settings.key_paragraph != blank %}
            <div class="key-content">
              {{ block.settings.key_paragraph }}
            </div>
            {% endif %}  
            </div>
            </div>
            {%- when 'table-of_content'  -%}
            <div class="table-of-content__main">
              <h4>Table of Contents</h4>
            <div class="table-of-content__inner">
              {% if block.settings.content_heading1 != blank %}
              <a href="{{ block.settings.content_url1 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading1 }}</a>
              {% endif %}
            {% if block.settings.content_heading2 != blank %}
              <a href="{{ block.settings.content_url2 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading2 }}</a>
              {% endif %}
            {% if block.settings.content_heading3 != blank %}
              <a href="{{ block.settings.content_url3 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading3 }}</a>
              {% endif %}
            {% if block.settings.content_heading4 != blank %}
              <a href="{{ block.settings.content_url4 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading4 }}</a>
              {% endif %}
            {% if block.settings.content_heading5 != blank %}
              <a href="{{ block.settings.content_url5 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading5 }}</a>
              {% endif %}
            {% if block.settings.content_heading6 != blank %}
              <a href="{{ block.settings.content_url6 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading6 }}</a>
              {% endif %}
            {% if block.settings.content_heading7 != blank %}
              <a href="{{ block.settings.content_url7 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading7 }}</a>
              {% endif %}
            {% if block.settings.content_heading8 != blank %}
              <a href="{{ block.settings.content_url8 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading8 }}</a>
              {% endif %}
            {% if block.settings.content_heading9 != blank %}
              <a href="{{ block.settings.content_url9 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading9 }}</a>
              {% endif %}
            {% if block.settings.content_heading10 != blank %}
              <a href="{{ block.settings.content_url10 }}"><img src="{{ 'play-button.png' | asset_url }}">{{ block.settings.content_heading10 }}</a>
              {% endif %}
            </div>  
            </div>
            {%- when 'image_with_details' -%}
        <div id="{{ block.settings.tbl_heading }}" class="image_with_description">
          {% if block.settings.heading != blank %}
          <h2 class="iwd_heading">{{ block.settings.heading }}</h2>
          {% endif %}
            {% if block.settings.enable_article_date %}
          <div class="iwd_subheading publish_date">
            <span> Written by  </span> {{ article.author | escape }} - <span>Published on </span> {{ article.published_at | date: "%b %d, %y" }}
          </div>
          {% endif %}
          {% if block.settings.subheading != blank %}
          <div class="iwd_subheading">{{ block.settings.subheading }}</div>
          {% endif %}
          {% if block.settings.image != blank %}
          <img class="iwd_image" src="{{ block.settings.image | img_url:"" }}" alt="image" {% if block.settings.description == blank %} style="padding-bottom:0;"{% endif %} >
           
           {% endif %}
          {% if block.settings.description != blank %}
          {{ block.settings.description }}
             {% endif %}
          {% if block.settings.text_with_img %}
            <div class="text_with_image">
              <div class="contents_data">
                <div class="inner_right_col">
                    {% if block.settings.left_col != blank %}
                         <div class="left_col mul_coll">{{ block.settings.left_col }}</div>
                      {% endif %}
                        {% if block.settings.right_col != blank %}
                            <div class="right_col mul_coll">{{ block.settings.right_col }}</div>
                          {% endif %}
                 </div>
               </div>
              <div class="image_data">
                {% if block.settings.product_img != blank %}
                     <img src="{{ block.settings.product_img | img_url:"" }}">
                {% endif %}
              </div>
            </div>
            {% endif %}
        </div>
        {%- when 'image_with_text' -%}
            <div id="{{ block.settings.tbl_heading }}" class="artcl_img_wth_txt">
                  {% if block.settings.heading != blank %}
                    <h2 class="iwd_heading">{{ block.settings.heading }}</h2>
                  {% endif %}
                  {% if block.settings.subheading != blank %}
                  <div class="iwd_subheading">{{ block.settings.subheading }}</div>
                  {% endif %}
              <div class="article_data">
                 {% if block.settings.article_content != blank %}
                         <div class="article_content_data">{{ block.settings.article_content }}</div>
                  {% endif %}
               
              <div class="article_image_data">
                {% if block.settings.artcile_img != blank %}
                     <img src="{{ block.settings.artcile_img | img_url:"" }}">
                {% endif %} 
                 {% if block.settings.artcile_img2 != blank %}
                     <img src="{{ block.settings.artcile_img2 | img_url:"" }}">
                {% endif %}
              </div>
            </div>
          </div>
             {%- when 'Multicolumn' -%}
            <div class="multi_column">
               {% if block.settings.heading != blank %}
                  <h2 class="iwd_heading">{{ block.settings.heading }}</h2>
                {% endif %}
              <div class="inner_multi_column">
                {% for i in (1..3) %}
                  {% capture image %}image_{{ forloop.index }}{% endcapture %}
                  {% capture heading %}heading_{{ forloop.index }}{% endcapture %}
                  {% capture description %}description_{{ forloop.index }}{% endcapture %}
                <div class="multicolumn_content">
                  {% if block.settings[image] != blank %}
                      <img class="multicol_img" src="{{ block.settings[image] | img_url:"" }}" alt="image">
                  {% else %}
                    {{ 'product-1' | placeholder_svg_tag }}
                  {% endif %}
                  {% if block.settings[heading] != blank %}
                      <div class="multicolum_heading">{{ block.settings[heading] }}</div>
                  {% endif %}
                  {% if block.settings[description] != blank %}
                      {{ block.settings[description]  }}
                    {% endif %}
                </div>
                   {% endfor %}
              </div>
            </div>
            
             {%- when 'promotion_banner' -%}
            <div class="promotion_banner">
              <div class="inner_promotion_banner">
            {% if block.settings.title != blank %}
              <div class="promotion_heading">{{ block.settings.title }}</div>
              {% endif %}
            {% if block.settings.btn_text != blank %}
              <div class="promotion_btn">
                <a href="{{ block.settings.link }}">{{ block.settings.btn_text }}</a>
              </div>
              {% endif %}
              </div>
            </div>
            
